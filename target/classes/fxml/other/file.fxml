<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" fx:id="rootPane" prefHeight="360.0" prefWidth="150.0"
            xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.nicenote.ui.other.FileController">
    <VBox prefHeight="300.0" prefWidth="200.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
          AnchorPane.topAnchor="0.0">
        <padding>
            <Insets top="20.0"/>
        </padding>
        <Label onMouseClicked="#handleNewLabelOnMouseClicked" prefHeight="40.0" prefWidth="150.0" text="New">
            <padding>
                <Insets left="30.0"/>
            </padding>
            <styleClass>
                <String fx:value="file_label"/>
                <String fx:value="file_box"/>
            </styleClass>
        </Label>
        <Label onMouseClicked="#handleOpenLabelOnMouseClicked" prefHeight="40.0" prefWidth="150.0" text="Open">
            <padding>
                <Insets left="30.0"/>
            </padding>
            <styleClass>
                <String fx:value="file_label"/>
                <String fx:value="file_box"/>
            </styleClass>
        </Label>
        <Label onMouseClicked="#handleSaveLabelOnMouseClicked" prefHeight="40.0" prefWidth="150.0" text="Save">
            <padding>
                <Insets left="30.0"/>
            </padding>
            <styleClass>
                <String fx:value="file_label"/>
                <String fx:value="file_box"/>
            </styleClass>
        </Label>
        <Label onMouseClicked="#handleSaveAsLabelOnMouseClicked" prefHeight="40.0" prefWidth="150.0" text="Save As">
            <padding>
                <Insets left="30.0"/>
            </padding>
            <styleClass>
                <String fx:value="file_label"/>
                <String fx:value="file_box"/>
            </styleClass>
        </Label>
        <Label onMouseClicked="#handlePageSetUpLabelOnMouseClicked" prefHeight="40.0" prefWidth="150.0"
               text="Page Setup">
            <padding>
                <Insets left="30.0"/>
            </padding>
            <styleClass>
                <String fx:value="file_label"/>
                <String fx:value="file_box"/>
            </styleClass>
        </Label>
        <Label onMouseClicked="#handlePrintLabelOnMouseClicked" prefHeight="40.0" prefWidth="150.0" text="Print">
            <padding>
                <Insets left="30.0"/>
            </padding>
            <styleClass>
                <String fx:value="file_label"/>
                <String fx:value="file_box"/>
            </styleClass>
        </Label>
        <Label onMouseClicked="#handleHelpLabelOnMouseClicked" prefHeight="40.0" prefWidth="150.0" text="Help">
            <padding>
                <Insets left="30.0"/>
            </padding>
            <styleClass>
                <String fx:value="file_label"/>
                <String fx:value="file_box"/>
            </styleClass>
        </Label>
    </VBox>
</AnchorPane>
